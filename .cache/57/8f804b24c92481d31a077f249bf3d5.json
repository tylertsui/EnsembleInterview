":56},"source":"StarshipDetails.js","original":{"line":39,"column":4}},{"generated":{"line":86,"column":0},"source":"StarshipDetails.js","original":{"line":85,"column":1}},{"generated":{"line":86,"column":1},"source":"StarshipDetails.js","original":{"line":8,"column":0}},{"name":"StarshipDetails","generated":{"line":88,"column":15},"source":"StarshipDetails.js","original":{"line":87,"column":15}}],"sources":{"StarshipDetails.js":"import axios from \"axios\";\r\nimport React, { useState, useEffect } from \"react\";\r\nimport { useLocation } from \"react-router\";\r\n\r\nimport { film_link, character_link } from \"./String\";\r\nimport LinkBlock from \"./LinkBlock\";\r\n\r\nconst StarshipDetails = () => {\r\n\r\n    const location = useLocation();\r\n    const { toDisplay } = location.state;\r\n    const starship = toDisplay;\r\n\r\n    const [pilots, setPilots] = useState([]);\r\n    const [films, setFilms] = useState([]);\r\n\r\n    useEffect(()=> {\r\n        let pilotPromises = starship.pilots.map((pilot) => {\r\n            return axios.get(pilot);\r\n        });\r\n\r\n        let filmPromises = starship.films.map((film) => {\r\n            return axios.get(film);\r\n        });\r\n\r\n        axios.all(pilotPromises).then((results) => {\r\n            setPilots(results.map((result) => {\r\n                return result.data;\r\n            }));\r\n        });\r\n\r\n        axios.all(filmPromises).then((results) => {\r\n            setFilms(results.map((result) => {\r\n                return result.data;\r\n            }));\r\n        });\r\n    }, []);\r\n\r\n    return (\r\n        <div>\r\n            <div>\r\n                Name: {starship.name}\r\n            </div>\r\n            <div>\r\n                Model: {starship.model}\r\n            </div>\r\n            <div>\r\n                Class: {starship.starship_class}\r\n            </div>\r\n            <div>\r\n                Crew: {starship.crew}\r\n            </div>\r\n            <div>\r\n                Cargo Capacity:{starship.cargo_capacity}\r\n            </div>\r\n            <div>\r\n                Manufacturer: {starship.manufacturer}\r\n            </div>\r\n            <div>\r\n                Max Atmosphering Speed: {starship.max_atmoshpering_speed}\r\n            </div>\r\n            <div>\r\n                Length: {starship.length}\r\n            </div>\r\n            <div>\r\n                Passengers: {starship.passengers}\r\n            </div>\r\n            <div>\r\n                Cost in Credits: {starship.cost_in_credits}\r\n            </div>\r\n            <div>\r\n                Known Pilots : {\r\n                    pilots.length > 0 ? (pilots.map((pilot, index) => {\r\n                        return <LinkBlock key={index} data={pilot} path={character_link} name={pilot.name} />\r\n                    })) : <div>NA</div>}\r\n            </div>\r\n            <div>\r\n                Film Appearances: {\r\n                    films.length > 0 ? (films.map((film, index) => {\r\n                        return <LinkBlock key={index} data={film} path={film_link} name={film.title} />\r\n                    })) : <div>NA</div>}\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default StarshipDetails;"},"lineCount":null}},"error":null,"hash":"afd12b569312b981031dece9bc0a62a4","cacheData":{"env":{}}}