ginal":{"line":39,"column":12}},{"generated":{"line":79,"column":5},"source":"components/FilmDisplay.js","original":{"line":30,"column":8}},{"generated":{"line":79,"column":6},"source":"components/FilmDisplay.js","original":{"line":29,"column":4}},{"generated":{"line":80,"column":0},"source":"components/FilmDisplay.js","original":{"line":46,"column":1}},{"generated":{"line":80,"column":1},"source":"components/FilmDisplay.js","original":{"line":11,"column":0}},{"name":"FilmDisplay","generated":{"line":82,"column":15},"source":"components/FilmDisplay.js","original":{"line":48,"column":15}}],"sources":{"components/FilmDisplay.js":"import React, { useState, useEffect } from \"react\";\r\nimport axios from \"axios\";\r\n\r\nimport { film_endpoint, film_link } from \"../strings/String\";\r\nimport LinkBlock from \"../common/LinkBlock\";\r\n\r\n\r\n// Fetches and displays current films in SWAPI\r\n// Search bar continually updates query, which triggers the useEffect to perform a fetch using\r\n// SWAPI's own search query end point and displays results back on screen\r\nconst FilmDisplay = () => {\r\n\r\n    const [display, setDisplay] = useState([]);\r\n    const [query, setQuery] = useState(\"Star Wars\");\r\n\r\n    useEffect(() => {\r\n        if (query == \"Star Wars\" || query ==\"\") {\r\n            axios.get(`${film_endpoint}`).then((e) => {\r\n                setDisplay(e.data.results);\r\n            });\r\n        } else {\r\n            axios.get(`${film_endpoint}?search=${query}`).then((e) => {\r\n                setDisplay(e.data.results);\r\n            });\r\n        }\r\n\r\n    }, [query]);\r\n\r\n    return (\r\n        <div>\r\n            <div id=\"search-param\">\r\n                <form onSubmit=\"\">\r\n                    <label htmlFor=\"search-bar\">\r\n                        Search: \r\n                        <input type=\"text\" id=\"search-bar\" value={query} onChange={e => setQuery(e.target.value)}></input>\r\n                    </label>\r\n                </form>            \r\n            </div>\r\n            <div>\r\n                {display.map((film, index) => {\r\n                    return <LinkBlock key={index} data={film} path={film_link} name={film.title} />\r\n                })}\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default FilmDisplay;"},"lineCount":null}},"error":null,"hash":"bc3e548a708a0b3b8b52a3ccba48f5c2","cacheData":{"env":{}}}