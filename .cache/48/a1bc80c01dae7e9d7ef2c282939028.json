omises = vehicle.films.map((film) => {\r\n            return axios.get(film);\r\n        });\r\n\r\n        axios.all(pilotPromises).then((results) => {\r\n            setPilots(results.map((result) => {\r\n                return result.data;\r\n            }));\r\n        });\r\n\r\n        axios.all(filmPromises).then((results) => {\r\n            setFilms(results.map((result) => {\r\n                return result.data;\r\n            }));\r\n        });\r\n    }, []);\r\n\r\n    return (\r\n        <div>\r\n            <div>\r\n                Name: {vehicle.name}\r\n            </div>\r\n            <div>\r\n                Class: {vehicle.vehicle_class}\r\n            </div>\r\n            <div>\r\n                Model: {vehicle.model}\r\n            </div>\r\n            <div>\r\n                Passengers: {vehicle.passengers}\r\n            </div>\r\n            <div>\r\n                Cargo Capacity: {vehicle.cargo_capacity}\r\n            </div>\r\n            <div>\r\n                Crew: {vehicle.crew}\r\n            </div>\r\n            <div>\r\n                Cost In Credits: {vehicle.cost_in_credits}\r\n            </div>\r\n            <div>\r\n                Manufacturer: {vehicle.manufacturer}\r\n            </div>\r\n            <div>\r\n                Max Atmosphering Speed: {vehicle.max_atmosphering_speed}\r\n            </div>\r\n            <div>\r\n                Known Pilots : {\r\n                    pilots.length > 0 ? (pilots.map((pilot, index) => {\r\n                        return <LinkBlock key={index} data={pilot} path={character_link} name={pilot.name} />\r\n                    })) : <div>NA</div>}\r\n            </div>\r\n            <div>\r\n                Film Appearances: {\r\n                    films.length > 0 ? (films.map((film, index) => {\r\n                        return <LinkBlock key={index} data={film} path={film_link} name={film.title} />\r\n                    })) : <div>NA</div>}\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default VehicleDetails;"},"lineCount":null}},"error":null,"hash":"9f6098144d503febbf4e2afa989c140f","cacheData":{"env":{}}}